<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level (%F:%L) %msg%n" />
		</Console>
	</appenders>

	<loggers>
		<root level="debug">
			<appender-ref ref="Console" level="debug" />
		</root>
	</loggers>
</configuration>